{
  "name": "doxy-directory",
  "version": "1.0.0",
  "description": "Directory of MUSC Doxy.me rooms and URLs",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",
    "client": "npm start --prefix client",
    "clientinstall": "npm install --prefix client",
    "dev": "concurrently \"npm run start\" \"npm run client\"",
    "render": "concurrently \"npm run start\" \"npm run client\"",
    "clientbuild": "npm run build --prefix client",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "render-postbuild": "NPM_CONFIG_PRODUCTION=false npm clientinstall && clientbuild"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^6.0.2",
    "nodemon": "^2.0.7"
  },
  "dependencies": {
    "json-server": "0.17.1"
  }
}
